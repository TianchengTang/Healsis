  body(style='background-color: #F6F6F6')
    // Header
    nav.navbar.navbar-inverse.navbar-fixed-top
      .container-fluid(style='background-color: #FFF')
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
            span.sr-only Toggle navigation
            |             
            span.icon-bar
            |             
            span.icon-bar
            |             
            span.icon-bar
          | &#x9;&#x9;  
          img(style='float:left; padding-top:5px; margin-right:20px', src='../images/logo.png', height='15%', width='15%')
          |           
          a.navbar-brand(href='#') Healsis
        |         
        #navbar.navbar-collapse.collapse
          a.btn.btn-sm.btn-default(href='../index.jade', type='button', style='font-size:14px; padding-left:15px; padding-right:15px; margin-top:10px; margin-right:20px; float:right') Logout
    |     
    .container-fluid
      .row
        // Navigation Bar
        .col-sm-3.col-md-2.sidebar
          .nav-sidebar-subtitle Data Base
          |   
          |           
          ul.nav.nav-sidebar
            li.active
              a(href='../dashboard.jade') Client Data Inquiry
            |             
            li
              a(href='patient_data_update.jade') Client Data Update
            |             
            li
              a(href='neighbor_stat.jade') Neighborhood Statistics
            |             
            li
              a(href='staff_contact.jade') Staff Contact Info
            | &#x9;&#x9;&#x9;
            li
              a(href='my_profile.jade') My Profile
          | &#x9;&#x9;  
          .nav-sidebar-subtitle Task Center
          |  
          |           
          ul.nav.nav-sidebar
            li
              a(href='assign_tasks.jade') Assgin Tasks
            |             
            li
              a(href='my_tasks.jade') My Tasks
            |             
            li
              a(href='generate_reg.jade') Generate Reg Key
          | &#x9;&#x9;  
          .nav-sidebar-subtitle Personal Health
          |           
          ul.nav.nav-sidebar
            li
              a(href='#') My H-Graph
        // Main Frame
        .col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-2.main
          h1.page-header Assign Tasks
          // Section Title
          // <h2 class="sub-header">Section title</h2>
          .row
            // Input Column 1/2
            .col-md-5
              .row
                label.item-name.col-md-5 Assignee Position
                | &#x9;&#x9;&#x9;&#x9;
                select#txtSel_AssigneePos.form-control.select-list.col-md-7
                  option Doctor
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Social Worker
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Developer
              | &#x9;&#x9;&#x9;  
              .row
                label.item-name.col-md-5 Assignee Name
                | &#x9;&#x9;&#x9;&#x9;
                select#txtSel_AssigneeName.form-control.select-list.col-md-7
                  option Name 1
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Name 2
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Name 3
              | &#x9;&#x9;&#x9;  
              | &#x9;&#x9;&#x9;  
              .row
                label.item-name.col-md-5 Area
                | &#x9;&#x9;&#x9;&#x9;
                select#txtSel_Area.form-control.select-list.col-md-7
                  option Area 1
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Area 2
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Area 3
              | &#x9;&#x9;&#x9;  
              .row
                label.item-name.col-md-5 Location
                | &#x9;&#x9;&#x9;&#x9;
                select#txtSel_Location.form-control.select-list.col-md-7
                  option Location 1
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Location 2
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Location 3
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Location 4
                  | &#x9;&#x9;&#x9;&#x9;&#x9;
                  option Location 5
              | &#x9;&#x9;&#x9;  
              | &#x9;&#x9;&#x9;  
              .row
                label.item-name.col-md-5 Patient ID
                | &#x9;&#x9;&#x9;&#x9;
                input#txtFd_PatientID.form-control.input-field.col-md-7
            // Input Column 2/2
            .col-md-5
              .row
                label.item-name.col-md-5 Assigned Date
                | &#x9;&#x9;&#x9;&#x9;
                input#txtFd_LastVisitAfter.form-control.input-field.col-md-7(placeholder=' mm/dd/yy')
              | &#x9;&#x9;&#x9;  
              .row
                label.item-name.col-md-5 Task Category
                | &#x9;&#x9;&#x9;&#x9;
                select#txtSel_Location.form-control.select-list.col-md-7
                  option Update Patient Data
          // center aligning has no effect if the width property is not set or set to 100%
          #btn_Assign.row(style='margin:auto; width:30%; margin-top:5px')
            button.btn.btn-sm.btn-default.btn-big(type='button', onclick='clickAssign()') Assign
